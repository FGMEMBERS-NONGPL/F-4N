          <property>aero/function/kCLge</property>
          <table>
            <independentVar lookup="row">aero/alpha-rad</independentVar>
            <tableData>
              -0.20 -0.64
               0.00  0.08
               0.26  1.00
               0.40  0.95
               0.60  0.40
               1.30  0.05
            </tableData>
          </table>
      </product>
    </function>

